@inherits ChampionshipRankingEditBase
@page "/ChampionshipRanking/Edit/{id}"
@attribute [Authorize(Policy = "indexes-control")]

<div class="page-title mt-3 mb-4"> @Localizer["Edit"] @Localizer["ChampionshipRanking"] </div>

<EditForm Model="EntityItem" OnValidSubmit="HandelSubmit">

    <FluentValidationValidator />

    <ChampionshipRankingForm ChampionshipRanking="EntityItem" IsEdit="true" />

    <SubmitBtn Text="Save" Busy="EditBusy" />

</EditForm>

<Btn ClassList="btn-sm btn-comp-s" Text="Back To List" ClickCallBack="@(() => NavigationManager.NavigateTo("/ChampionshipsRanking/List"))" />