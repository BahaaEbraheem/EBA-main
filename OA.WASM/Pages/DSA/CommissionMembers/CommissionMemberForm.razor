@using OA.Domin.DSA
@using OA.Domin.Settings




<div class="form-group row">
    <div class="col-sm-6 select-col">

        <MInputSelect @bind-Value="CommissionMember.AdministrationBoardId" For="@(() => CommissionMember.AdministrationBoardId)" Items="administrationboardsItems" Id="AdministrationBoardId" EntityType="typeof(CommissionMember)" PropName="AdministrationBoardId" />
    </div>
    <div class="col-sm-6 select-col">

        <MInputSelect @bind-Value="CommissionMember.CommissionId" For="@(() => CommissionMember.CommissionId)" Items="CommissionsItems" Id="CommissionId" EntityType="typeof(CommissionMember)" PropName="CommissionId" />
    </div>

</div>
<div class="form-group row">

    <div class="col-sm-6">
        <MInputDate Type="date" @bind-Value="CommissionMember.EndDate" For="@(() => CommissionMember.EndDate)" Id="EndDate" EntityType="typeof(CommissionMember)" PropName="EndDate" />
    </div>
    <div class="col-sm-6">
        <MInputDate Type="date" @bind-Value="CommissionMember.StartDate" For="@(() => CommissionMember.StartDate)" Id="StartDate" EntityType="typeof(CommissionMember)" PropName="StartDate" />
    </div>
  
</div>

<div class="form-group row">


    <div class="col-sm-12">
        <MInputText @bind-Value="CommissionMember.Description" For="@(() => CommissionMember.Description)" Id="Description" EntityType="typeof(CommissionMember)" PropName="Description" Active="@Active" />
    </div>
</div>







