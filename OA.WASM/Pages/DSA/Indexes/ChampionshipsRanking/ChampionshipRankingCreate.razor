@inherits ChampionshipRankingCreateBase
@page "/ChampionshipRanking/Create"
@attribute [Authorize(Policy = "indexes-control")]

<div class="page-title mt-3 mb-4"> @Localizer["Create"] @Localizer["ChampionshipRanking"] </div>

<EditForm Model="EntityItem" OnValidSubmit="HandelSubmit">

    <FluentValidationValidator />

    <ChampionshipRankingForm ChampionshipRanking="EntityItem" />

    <SubmitBtn Text="Create" Busy="CreateBusy" />

</EditForm>

<Btn ClassList="btn-sm btn-comp-s" Text="Back To List" ClickCallBack="@(() => NavigationManager.NavigateTo("/ChampionshipsRanking/List"))" />
